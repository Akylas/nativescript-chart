{
    "name": "nativescript-chart",
    "version": "0.0.26",
    "description": "Nativescript plugin for Charts",
    "scripts": {
        "tsc": "tsc -skipLibCheck -d",
        "clean": "rimraf ./plugin/**/*.d.ts ./plugin/**/*.js ./plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
        "build": "cp README.md plugin/ && rm -f .tsbuildinfo && npm run tsc",
        "demo.ios": "npm i && npm run tsc && cd demo && tns run ios",
        "demo.android": "npm i && npm run tsc && cd demo && tns run android",
        "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
        "demo.ios.vue": "npm i && npm run tsc && cd demo_vue && tns run ios",
        "demo.android.vue": "npm i && npm run tsc && cd demo_vue && tns run android",
        "clean.demo.vue": "rimraf demo_vue/hooks demo_vue/node_modules demo_vue/platforms",
        "plugin.watch.tsc": "npm run tsc -- -w",
        "plugin.watch": "npm run plugin.watch.tsc",
        "publish": "npm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-chart/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Akylas/nativescript-chart",
    "readmeFilename": "README.md",
    "devDependencies": {
        "@angular/core": "^8.2.14",
        "@commitlint/cli": "^9.1.2",
        "@commitlint/config-conventional": "^9.1.2",
        "@nativescript/core": "6.5.15",
        "@tweenjs/tween.js": "18.6.0",
        "@types/node": "^14.6.0",
        "@typescript-eslint/eslint-plugin": "3.10.1",
        "@typescript-eslint/parser": "3.10.1",
        "@types/tween.js": "^18.5.1",
        "cpy-cli": "^3.1.1",
        "downsample": "^1.3.0",
        "eslint": "7.7.0",
        "husky": "^4.2.5",
        "lerna": "^3.22.1",
        "nativescript-canvas": "3.0.10",
        "nativescript-gesturehandler": "^0.1.16",
        "nativescript-tween": "0.0.10",
        "npm-watch": "^0.7.0",
        "number-format.js": "^2.0.9",
        "prompt": "^1.0.0",
        "rimraf": "^3.0.2",
        "tns-platform-declarations": "^6.5.15",
        "typescript": "~4.0.2"
    },
    "bootstrapper": "nativescript-plugin-seed",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "dependencies": {}
}

{
    "name": "nativescript-chart",
    "version": "1.0.0",
    "description": "Nativescript plugin for Charts",
    "scripts": {
        "tsc": "tsc -skipLibCheck -d",
        "clean": "rimraf ./plugin/**/*.d.ts ./plugin/**/*.js ./plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
        "build.esm": "tsc -skipLibCheck --target es2015 --module esnext --removeComments --outDir build/esm && node ./build.esm.js",
        "build": "cp README.md plugin/ && rm -f .tsbuildinfo && npm run tsc && npm run build.esm",
        "demo.ios": "npm i && npm run tsc && cd demo && tns run ios",
        "demo.android": "npm i && npm run tsc && cd demo && tns run android",
        "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
        "plugin.watch.tsc": "npm run tsc -- -w",
        "plugin.watch": "npm run plugin.watch.tsc",
        "publish": "npm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-chart/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Akylas/nativescript-chart",
    "readmeFilename": "README.md",
    "devDependencies": {
        "@angular/core": "^8.2.14",
        "@commitlint/cli": "^8.3.4",
        "@commitlint/config-conventional": "^8.3.4",
        "@nativescript/core": "6.3.2",
        "@tweenjs/tween.js": "18.5.0",
        "@types/node": "^13.1.6",
        "@types/tween.js": "^17.2.0",
        "cpy-cli": "^3.0.0",
        "husky": "^4.0.7",
        "lerna": "^3.20.2",
        "nativescript-canvas": "2.1.24",
        "nativescript-gesturehandler": "^0.1.6",
        "nativescript-tween": "0.0.5",
        "npm-watch": "^0.6.0",
        "number-format.js": "^2.0.9",
        "prompt": "^1.0.0",
        "recursive-copy": "^2.0.10",
        "rimraf": "^3.0.2",
        "through2": "^3.0.1",
        "tns-platform-declarations": "^6.3.2",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "~3.7.4"
    },
    "bootstrapper": "nativescript-plugin-seed",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "dependencies": {
    }
}
